# Gemfile for health_test testing

source :rubygems

group :development, :test do
  gem 'rails', "~> 2.3.15"
  # rails requires rake >= 0.8.3 
  gem 'jeweler', '~> 1.8.4'
  gem 'shoulda', "~> 2.11.0"
  gem 'sqlite3', "~> 1.3.7"
  # required to run rake test:plugins
  gem 'ruby-prof', '>= 0.6.1'
end

