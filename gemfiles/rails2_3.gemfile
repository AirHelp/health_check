# Gemfile for health_test testing

source :rubygems

gem 'rails', "~> 2.3.15"
# rails requires rake >= 0.8.3 
if defined?(JRUBY_VERSION)
  gem 'activerecord-jdbc-adapter', :require => false
  gem 'jdbc-sqlite3', :require => false
else
  gem 'sqlite3', "~> 1.3.7"
end

group :development, :test do
  gem 'jeweler', '~> 1.8.4'
  gem 'shoulda', "~> 2.11.0"
end

